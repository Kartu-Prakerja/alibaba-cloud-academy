                section
                    //- div.heading-sticky
                        h4 
                            a(href="#accordionTentang")  Apa itu Beasiswa Sertifikasi Alibaba Cloud Academy by Prakerja?
                
                    .accordion.pds-accordion#accordionTentang  
                            
                        .accordion-item(data-aos="fade-up" data-aos-once="true" data-aos-delay="50") 
                            h5.accordion-header 
                                button.accordion-button(type="button" data-bs-toggle="collapse" data-bs-target="#collapseA1" aria-expanded="true" aria-controls="collapseA1")
